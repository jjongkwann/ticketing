# AWS
AWS_REGION=us-east-1

# DynamoDB Tables
DYNAMODB_SEATS_TABLE=ticketing-seats-prod
DYNAMODB_RESERVATIONS_TABLE=ticketing-reservations-prod
DYNAMODB_BOOKINGS_TABLE=ticketing-bookings-prod

# Redis
REDIS_ENDPOINT=ticketing-redis.abc123.cache.amazonaws.com:6379
REDIS_DB=0
REDIS_LOCK_TTL=30

# Server Ports
GRPC_PORT=50051
HTTP_PORT=8080

# Reservation
RESERVATION_TTL_MINUTES=10
RESERVATION_CLEANUP_INTERVAL_SECONDS=60

# Logging
LOG_LEVEL=info

# Environment
ENVIRONMENT=production
